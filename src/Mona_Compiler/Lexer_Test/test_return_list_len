[int] lis(int a){
     if(a == 1){
          var [int] list1 = [1,2];
          return (list1) ;
     }
     if(a == 0){
          var [int] list2 = [1,2,3,4];
          return (list2) ;
     }
     var [int] l = [];
     return (l);
}
main{
     var [int] a = lis(20);
     var int i = a.length();
     print i ;
     var [int] l = lis(0);
     i = l.length();
     print i ;
     var [int] k = lis(1);
     i = k.length();
     print i ;


}
