void helloWorld(string a,int v){
    var int i = 0 ;
    while(i <= v){
        print a ;
        i  = i + 1 ;
    }

}
int get([int] a){
    return (10) ;
}

void testRecursive(string a, int v){
    if (v > 0){
        print v;
        v = v - 1;
        testRecursive(a, v);
    }
    else{
        print v;
    }

}

main{
    var int a = 0 ;
    var [int] l =[1,2,3];
    a = get( l ) ;
    print a ;
    a = a + 1;
    print a ;
}
