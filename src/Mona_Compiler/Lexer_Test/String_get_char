main{
     var string h = "racecar";
     var int i = 0 ;
     var int len = h.len();
     var string word2 = "racecar";
     var bool palindrome = true ;

     while( i < len){
          var string l = h.getChar(i);
          var string k = word2.getChar(i);
          i = i + 1 ;
          if(k != l){
               palindrome = false ;
          }
          print l ;
          print k ;
     }
     print 10 ;
     print palindrome ;
}
